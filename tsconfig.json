{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": [
      "node"
    ],
    "skipLibCheck": true
  },
  "exclude": [
    "_backup_patch",
    "dist",
    "node_modules",
    "out",
    "playwright-report",
    "playwright.config.js",
    "playwright.config.ts",
    "release",
    "test-results"
  ]
}
